<h1>Home Page</h1>
<table class="table">
  <thead class="thead-dark">
    <tr>
      <th scope="col">Title</th>
      <th scope="col">Description</th>
    </tr>
  </thead>
  <tbody>
  <% @article. each do |a| %>
  	
    <tr>
      
      <td><%= a.title%></td>
      <td><%= a.description%></td>
      <td><%= link_to 'show',({:action=>'show',:id=>a.id}) %>
      <td><%= link_to 'edit',({:action=>'edit', :id=> a.id}) %>
      <td><%= link_to 'Delete', article_path(a), method: :delete, data: {confirm: "Are you sure?"} %></td>

     </tr>
     <% end %> 
  </tbody>
 </table>
<%= link_to 'create_customers',new_article_path %>

